{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "React boilerplate with Webpack.",
  "main": "index.js",
  "scripts": {
    "lint:js": "eslint src/ --ext .js --ext .jsx",
    "lint:js:fix": "eslint src/ --ext .js --ext .jsx --fix",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js:fix",
    "build": "npm run lint && webpack --config config/webpack.prod.js",
    "start": "webpack-dev-server --config config/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bruno02221/es-boilerplate.git"
  },
  "keywords": [
    "es",
    "ecmascript",
    "es6",
    "es7",
    "javascript",
    "boilerplate",
    "starter",
    "react"
  ],
  "author": "Bruno C. Couto (brunodev02221@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bruno02221/react-boilerplate/issues"
  },
  "homepage": "https://github.com/bruno02221/react-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "openurl": "^1.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "prop-types": "^15.6.0",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^4.6.0",
    "immutable": "^3.8.2",
    "normalize.css": "^7.0.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-textarea-autosize": "^5.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
